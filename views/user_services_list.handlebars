<section class="section">
  <div class="container">
    <h1 class="title has-text-centered">Upcoming Services</h1>
    <div class="box">
      <div class="content">
        <ul>
          {{#each services}}
            <li class="mb-2">
              <article class="media">
                <div class="media-content">
                  <div class="content">
                    <p>
                      <strong>{{this.date}}</strong> <small>{{this.service}}</small>
                    </p>
                  </div>
                </div>
              </article>
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</section>